c/components/utils/FileUpload.js",["200"],"import React, { useState } from 'react'\nimport Dropzone from 'react-dropzone'\nimport axios from 'axios'\n\n\nfunction FileUpload({uploadImages}) {\n    const [images,setImages]=useState([])\n\n    const onDrop = (files) => {\n        let formData = new FormData(); \n        const config = {\n            header: {'content-type': 'multipart/form-data'}\n        }\n        formData.append(\"file\", files[0])\n\n        axios.post('/api/product/saveImage',formData,config)\n        .then(res => {\n            if(res.data.success){\n                setImages([...images,res.data.filePath])\n                uploadImages([...images,res.data.filePath])\n            }else{\n                alert('failed save image')\n            }\n        })\n    }\n\n    const onDeleteClick = (image) => {\n        const currentIndex = images.indexOf(image)\n        let newImages = [...images]\n        newImages.splice(currentIndex,1)\n        setImages(newImages)\n        uploadImages(newImages)\n    }\n\n    return (\n        <div style={{display:'flex',justifyContent:'space-between'}} >\n            <Dropzone onDrop={onDrop}>\n                {({ getRootProps, getInputProps}) => (\n                    <div style={{\n                        width:300 , height: 240, border:'1px solid lightgray',\n                        display:'flex', alignItems:'center', justifyContent:'center'\n                    }}\n                    {...getRootProps()}>\n                        <input {...getInputProps()} />\n                    <span style={{fontSize:'5rem'}}> + </span>\n                    </div>\n                )}\n            </Dropzone>\n            <div style={{display:'flex',width:'350px',height:'240px',overflowX:'scroll'}}>\n                {images.map((image,index)=>(\n                    <div onClick={()=> onDeleteClick(image)} key={index}>\n                        <img style={{minWidth:'300px',width:'300px',height:'240px'}}\n                             src={`http://localhost:5000/${image}`} />\n                    </div>\n                ))}\n            </div>\n        </div>\n    )\n}\n\nexport default FileUpload\n","/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/utils/CarouselSlider.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/LandingPage/Sections/CheckBox.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/LandingPage/Sections/Datas.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/LandingPage/Sections/RadioBox.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/LandingPage/Sections/SearchFeature.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/ProductDetailPage/ProductDetailPage.js",["201"],"import axios from \"axios\";\nimport React, { useEffect, useState } from \"react\";\nimport { withRouter } from \"react-router-dom\";\nimport { Row, Col, Comment, Tooltip, Rate } from \"antd\";\nimport ProductImage from \"./Sections/ProductImage\";\nimport ProductInfo from \"./Sections/ProductInfo\";\nimport ReviewForm from \"./Sections/ReviewForm\";\nimport moment from \"moment\";\nimport { useSelector } from \"react-redux\";\n\nfunction ProductDetailPage({ match }) {\n  const [product, setProduct] = useState([]);\n  const user = useSelector((state) => state.user);\n  const {\n    params: { productId },\n  } = match;\n\n  useEffect(() => {\n    axios\n      .get(`/api/product/products_by_id?id=${productId}&type=single`)\n      .then((res) => {\n        setProduct(res.data);\n      })\n      .catch((err) => alert(err));\n  }, []);\n\n  const refreshRender = (data) => {\n    setProduct(data);\n  };\n\n  return (\n    <div style={{ width: \"100%\", padding: \"3rem 4rem\" }}>\n      <div style={{ display: \"flex\", justifyContent: \"center\" }}>\n        <h1>{product.title}</h1>\n      </div>\n\n      <br />\n\n      <Row gutter={[16, 16]}>\n        <Col lg={12} sm={24}>\n          {/* ProductImage */}\n          <ProductImage detail={product} />\n        </Col>\n        <Col lg={12} sm={24}>\n          {/* ProductInfo */}\n          <ProductInfo detail={product} />\n        </Col>\n      </Row>\n      <h2>Reviews</h2>\n      {user.userData?.isAuth && (\n        <ReviewForm refreshRender={refreshRender} productId={productId} />\n      )}\n      <div>\n        {product.reviews?.map((review) => (\n          <div key={review._id}>\n            <Comment\n              author={review.name}\n              actions={[\n                <Rate allowHalf disabled defaultValue={review.rating} />,\n              ]}\n              datetime={\n                <Tooltip\n                  title={moment()\n                    .subtract(1, \"days\")\n                    .format(\"YYYY-MM-DD HH:mm:ss\")}\n                >\n                  <span>{moment().subtract(1, \"days\").fromNow()}</span>\n                </Tooltip>\n              }\n              content={<p>{review.comment}</p>}\n            ></Comment>\n          </div>\n        ))}\n      </div>\n    </div>\n  );\n}\n\nexport default withRouter(ProductDetailPage);\n","/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/ProductDetailPage/Sections/ProductInfo.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/ProductDetailPage/Sections/ProductImage.js",["202"],"import React, { useEffect, useState } from \"react\";\nimport ImageGallery from \"react-image-gallery\";\n\nimport \"./styles.css\";\n\nfunction ProductImage({ detail }) {\n  const [Images, setImages] = useState([]);\n\n  useEffect(() => {\n    if (detail.images && detail.images.length > 0) {\n      let images = [];\n\n      detail.images.map((item) => {\n        images.push({\n          original: `http://localhost:5000/${item}`,\n          thumbnail: `http://localhost:5000/${item}`,\n        });\n      });\n      setImages(images);\n    }\n  }, [detail]);\n\n  return (\n    <div>\n      <ImageGallery items={Images} />\n    </div>\n  );\n}\n\nexport default ProductImage;\n","/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/CartPage/CartPage.js",["203"],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/CartPage/Sections/UserCardBlock.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/utils/Paypal.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/ProfilePage/ProfilePage.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/History/History.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/ProductDetailPage/Sections/ReviewForm.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/_reducers/product.js",["204"],"import { GET_PRODUCT_DETAIL } from \"../_actions/types\";\n\nfunction productReducer(state = {}, action) {\n  switch (action.type) {\n    default:\n      return state;\n  }\n}\n\nexport default productReducer;\n","/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/utils/ShippingForm.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/NavBar/Sections/ChatbotButton.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/_reducers/chatbot.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/Ctatbot/Chatbot.js",["205"],"import React, { useEffect, useRef } from \"react\";\nimport Axios from \"axios\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport Message from \"./Sections/Message\";\nimport { List, Avatar } from \"antd\";\nimport { RobotOutlined, SmileOutlined } from \"@ant-design/icons\";\nimport Card from \"./Sections/Card\";\nimport { saveMessage } from \"../../../_actions/chatbot\";\nimport styled from \"styled-components\";\n\nconst ChatbotWrapper = styled.div`\n  z-index: 100;\n  position: 