>\n    </ChatbotWrapper>\n  );\n}\n\nexport default Chatbot;\n","/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/Ctatbot/Sections/Card.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/Ctatbot/Sections/Message.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/_actions/chatbot.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/CheckoutPage/CheckoutPage.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/components/views/NavBar/Sections/ActionButtons.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/_reducers/user.js",[],"/Users/taerikim/Documents/web/johnAhn/shop-MERN/client/src/_actions/user.js",[],{"ruleId":"194","replacedBy":"195"},{"ruleId":"196","replacedBy":"197"},{"ruleId":"198","severity":1,"message":"199","line":60,"column":17,"nodeType":"200","endLine":60,"endColumn":128},{"ruleId":"194","replacedBy":"201"},{"ruleId":"196","replacedBy":"202"},{"ruleId":"203","severity":1,"message":"204","line":17,"column":17,"nodeType":"205","messageId":"206","endLine":17,"endColumn":25},{"ruleId":"203","severity":1,"message":"207","line":20,"column":10,"nodeType":"205","messageId":"206","endLine":20,"endColumn":20},{"ruleId":"208","severity":1,"message":"209","line":29,"column":6,"nodeType":"210","endLine":29,"endColumn":8,"suggestions":"211"},{"ruleId":"208","severity":1,"message":"212","line":33,"column":6,"nodeType":"210","endLine":33,"endColumn":21,"suggestions":"213"},{"ruleId":"208","severity":1,"message":"214","line":44,"column":6,"nodeType":"210","endLine":44,"endColumn":8,"suggestions":"215"},"no-native-reassign",["216"],"no-negated-in-lhs",["217"],"jsx-a11y/anchor-is-valid","The href attribute is required for an anchor to be keyboard accessible. Provide a valid, navigable address as the href value. If you cannot provide an href, but still need the element to resemble a link, use a button and change it with appropriate sty